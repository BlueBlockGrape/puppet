{
  "Hostname": "<%= @hostname %>",
  "GitHub": {
    "Branch": "<%= @github_branch %>",
    "OAuthToken": "<%= @github_oauth_token %>",
    "Secret": "<%= @github_webhook_secret %>"
  },
  "Slack": {
    "WebhookURL": "<%= @slack_webhook_url %>"
  },
<% if @newrelic_application_id != nil and @newrelic_api_key != nil -%>
  "NewRelic": {
    "ApplicationID": "<%= @newrelic_application_id %>",
    "ApiKey": "<%= @newrelic_api_key %>"
  }
<% else -%>
  "NewRelic": null
<% end -%>
}
